Funprog GET_ITM_COM(PS_VCRNUM, PI_VCRLIN)
  Value Char PS_VCRNUM
  Value Integer PI_VCRLIN

  Local Char ITM_COM(250)
  ITM_COM = ''


  If !clalev([F:YSOQ]): Local File SORDERQ     [F:YSOQ]  : Endif


    Read [F:YSOQ]SOQ0 = PS_VCRNUM;PI_VCRLIN;PI_VCRLIN
    If !fstat
      ITM_COM = [F:YSOQ]YITMCOMP
    Endif
End ITM_COM

####################################################################################
Funprog GET_ITM_COM_NEW(PS_PRHNUM, PI_ITM, PS_SDH)
  Value Char PS_PRHNUM, PI_ITM, PS_SDH

  Local Char ITM_COM(250)
  ITM_COM = ''

  If !clalev([F:YSOQ]): Local File SORDERQ     [F:YSOQ]  : Endif
  If !clalev([F:YSDD]): Local File SDELIVERYD     [F:YSDD]  : Endif

  Filter [F:YSDD] Where [F:YSDD]SDHNUM = PS_SDH and [F:YSDD]PRHNUM = PS_PRHNUM and [F:YSDD]ITMREF = PI_ITM
    For [F:YSDD]
      Read [F:YSOQ]SOQ0 = [F:YSDD]SOHNUM;[F:YSDD]SOPLIN;[F:YSDD]SOQSEQ
        If !fstat
          ITM_COM = [F:YSOQ]YITMCOMP
        Endif
    Next

End ITM_COM
