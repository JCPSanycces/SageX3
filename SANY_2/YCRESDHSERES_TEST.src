#**
#* Función encargada de generar un DESADV a partir de la información de una o varias entregas
#*
#* @param PALBARAN
#* @param PSDHDEB
#* @param PSDHFIN
#* @param PBPCDEB
#* @param PBPCFIN
#* @param PDATEDEB
#* @param PDATEFIN
#* @param PDUPLICADO
#*!
Subprog ZCREINVSERES_TEST(PCITA,PALBARAN,PVALE, PSDHDEB, PSDHFIN, PBPCDEB, PBPCFIN, PDATEDEB, PDATEFIN, PDUPLICADO, PI_FICHERO_ABIERTO, I, J,B,P)
# DESADV
  Value Char PCITA
  Value Char PALBARAN
  Value Char PVALE
  Value Char PSDHDEB, PSDHFIN, PBPCDEB, PBPCFIN
  Value Integer PDUPLICADO
  Value Date PDATEDEB, PDATEFIN
  Variable Integer PI_FICHERO_ABIERTO
  Variable Integer I
  Variable Integer J
  Variable Integer B # SUMATORIO BULTOS
  Variable Integer P # SUMATORIO PALETS

  Local Char LCRITERIO(250)
  #--
  Gosub OUVRE

    #-- EMBALAJES
    Local Char LPRHNUM
    Local Integer LPRHLIN
    Local Integer STAT : STAT = 1

   For [F:YZCIT2] Where [F:YZCIT2]ZCITANUM = PCITA

      Gosub DEFINE_FIC
      Gosub ECR_RECTL
      If PI_FICHERO_ABIERTO = 0
        Gosub ECR_SINCC
          For WCAL = 1 To 4
            Gosub ECR_SINCP
          Next
      Endif

      LPRHNUM = PVALE
      If I < 1 and  PI_FICHERO_ABIERTO = 0 Then
           Gosub ECR_SINCEMB_L0 # -- LINEA TOTALES PALETS - 08
           Gosub ECR_SINCEMB_L1 # -- LINEA TOTALES BULTO  - C
      Endif

      Filter [F:YSPK] Where VCRNUM = [F:YZCIT2]PRHNUM and PACNUM = [F:YZCIT2]PACNUM  and ZCITANUM = PCITA
          For [F:YSPK]
              Gosub ECR_SINCEMB # CABECERA DEL BULTO

              For [F:YPACKD2] Where [F:YPACKD2]VCRNUM = [F:YZCIT2]PRHNUM and [F:YPACKD2]PACNUM = [F:YZCIT2]PACNUM and ([F:YPACKD2]LINTYP <> 3 and [F:YPACKD2]LINTYP <> 6 and [F:YPACKD2]LINTYP <> 7)
                  Gosub ECR_SINCL   # -- LINEAS BULTO
                  Gosub ECR_SINCOBS # -- LINEAS BULTO
                  Gosub ECR_SINCQTY # -- LINEAS BULTO

                  # CAMBIO ESTADO DE ENVIO EDI DE LA ENTREGA
                  Read [F:YSDH2]SDH0 = [F:YPACKD2]SDHNUM
                    If !fstat #and [F:YSDH2]YENVIOEDI = 1
                        [F:YSDH2]YENVIOEDI = 2
                        [F:YSDH2]YFECHAEDI = date$
                        Trbegin [F:YSDH2]
                        Rewrite [F:YSDH2]
                        If !fstat Then
                           Commit
                        Else
                           Rollback
                        Endif
                    Endif
              Next
          Next
      PI_FICHERO_ABIERTO = 1
      Filter [F:YSPK]
  Next
    Openo Using [ZCRE] # cerramos fichero
End

###########################################################################################
