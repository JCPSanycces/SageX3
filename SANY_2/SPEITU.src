Subprog AS_NBITU

NOL = nolign-1
[M]XLIGITU = nolign-1
Gosub CHARGE_ITU From SUBITU
[M:ITU]XZETICLI    = [M:ITU]ZETICLI(NOL)
Affzo [M:ITU]XZETICLI
Raz GBOUT1, GBOUT2
If [M:ITU]BPCNUM(NOL) <> ""
