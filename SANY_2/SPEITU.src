Subprog AM_XZETICLI(VALEUR)
Variable Integer VALEUR
  [M:ITU]ZETICLI([M:ITU]XLIGITU) = VALEUR
  [M:ITU]UPDDAT([M:ITU]XLIGITU) = date$
  [M:ITU]UPDUSR([M:ITU]XLIGITU) = GUSER
  [M:ITU]FLMOD([M:ITU]XLIGITU) = 1
End


