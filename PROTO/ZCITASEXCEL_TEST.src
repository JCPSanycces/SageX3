Funprog PESO_EMPAQUETADO(YSOH, YPRH, CITA)
Value Char YSOH, YPRH, CITA
Local Char LREQSQL(250)(10)
Local Decimal TOT_PESO : TOT_PESO = 0

   Raz LREQSQL
   LREQSQL(0)=" SELECT D.ORINUM_0, O.CUSORDREF_0, Z.PRHNUM_0, S.YBRWEI_0 From " + nomap + ".YSPACKD P "
   LREQSQL(1)=" INNER JOIN " + nomap + ".ZCITASTL Z ON Z.PACNUM_0=P.PACNUM_0 and Z.PRHNUM_0=P.VCRNUM_0"
   LREQSQL(2)=" INNER JOIN " + nomap + ".STOPRED D  ON D.PRHNUM_0=P.VCRNUM_0 and D.PRELIN_0=P.VCRLIN_0 "
   LREQSQL(3)=" INNER JOIN " + nomap + ".SORDER O ON O.SOHNUM_0=D.ORINUM_0 "
   LREQSQL(4)=" INNER JOIN " + nomap + ".YSPACK S ON P.VCRNUM_0 = S.VCRNUM_0 AND P.PACNUM_0 = S.PACNUM_0  "
   LREQSQL(5)=" Where Z.ZCITANUM_0 ='" + CITA + "'"
   LREQSQL(6)=" GROUP By D.ORINUM_0, O.CUSORDREF_0, Z.PRHNUM_0, S.YBRWEI_0 "

   For (Char SOHNUM, Char CUSORDREF, Char PRHNUM, Decimal PESO ) From '5' Sql LREQSQL(0..9) As [YAPP]
    If [YAPP]CUSORDREF = YSOH and [YAPP]PRHNUM = YPRH
      TOT_PESO += [YAPP]PESO
    Endif
   Next

End TOT_PESO
