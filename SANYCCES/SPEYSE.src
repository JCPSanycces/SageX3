$ACTION
Case ACTION
  When "DEBUT" : Gosub DEBUT
Endcase

Return

#---------------------------------------------------------------------------------------------------#
#                                           LABELS                                                  #
#---------------------------------------------------------------------------------------------------#
$DEBUT
  If !clalev([YHIS]) : Local File YHISTSEG [YHIS] : Endif

  nolign = 1
  [M:YSE]NBLIG = 0

  Filter [YHIS] Where YREFERENCIA = [M:PRH0]PRHNUM Order By CREDATTIM
  For [YHIS]
    [M:YSE] = [YHIS]

    nolign +=1
    [M:YSE]NBLIG += 1
  Next
  Filter [YHIS]

  Affzo [M:YSE]

  Close Local File [YHIS]
Return

