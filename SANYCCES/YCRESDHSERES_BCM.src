        For [F:YSPK]

          #Gosub ECR_SINCEMB #-- -- LINEA POR CADA PALET O BULTO ENVIADO -- TAMBIÉN LLAMADO CONTENEDOR secuencia de embalajes
        Local Integer YCOMPRO: YCOMPRO =0
