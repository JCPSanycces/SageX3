Funprog GET_ITM_COM(PS_VCRNUM, PI_VCRLIN)
  Value Char PS_VCRNUM
  Value Integer PI_VCRLIN

  Local Char ITM_COM(250)
  ITM_COM = ''

  If !clalev([F:YSTD]): Local File STOPRED     [F:YSTD]  : Endif
  If !clalev([F:YSOQ]): Local File SORDERQ     [F:YSOQ]  : Endif

  Read [F:YSTD]PRE0 = PS_VCRNUM;PI_VCRLIN
  If !fstat
    Read [F:YSOQ]SOQ0 = [F:YSTD]ORINUM; [F:YSTD]ORILIN; [F:YSTD]ORISEQ
    If !fstat
      ITM_COM = [F:YSOQ]YITMCOMP
    Endif
  Endif

  Read

End ITM_COM

